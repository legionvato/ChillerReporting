    {
      "title": "Final Status",
      "items": [
        { "id": "final_operation", "label": "Unit returned to normal operation", "type": "status+notes" },
        { "id": "final_followup", "label": "Follow-up required (describe in notes)", "type": "status+notes" }
      ]
