      "items": [
        { "id": "visual_leaks", "label": "No evidence of oil/refrigerant leaks", "type": "status+notes" },
        { "id": "visual_vibration", "label": "No abnormal noise/vibration", "type": "status+notes" }
      ]
    },
    {
